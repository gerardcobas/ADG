    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title" style="color: #3a87ad">Informació detallada</h4>
        </div>
      	<div class="modal-body">
      		<div class="imprimir" id="detall-parroquies">
	      		<h4>Parròquia</h4>
	      		{% if info.nom is not null and info.nom is not empty %}<h5 style="color: #3a87ad">Nom de la parròquia</h5><p>{{ info.nom }}</p>{% endif %}
	      		{% if info.descripcio is not null and info.descripcio is not empty %}<h5 style="color: #3a87ad">Descripció</h5><p>{{ info.descripcio }}</p>{% endif %}
	      		{% if info.bibliografia is not null and info.bibliografia is not empty %}<h5 style="color: #3a87ad">Nom de la parròquia</h5><p>{{ info.bibliografia }}</p>{% endif %}
	
	      		<div class="space10"></div>
	      		<h4>Arxius dipositats</h4>
	      		
	      		<ul class="list-group">
					{% for a in arxius %}
						<li class="list-group-item">
						    <h5 class="list-group-item-heading">{{ a.nodac }}</h5>
						    {{ a.titol }}
						</li>
					{% else %}	
						<div class="alert alert-warning" style="margin-top:10px">
							{{ 'parroquies.avis' | trans }}
						</div>   
		            {% endfor %}
				</ul>
      		</div>
        </div>
        
        <div class="modal-footer">
        	<button class="btn btn-default" onclick="imprimir('#detall-parroquies')" ><i class="icon-print"></i>&nbsp;Imprimir</button>
          	<button class="btn btn-default" data-dismiss="modal">Tancar</button>
        </div>

      </div><!-- /.modal-content -->
    </div><!-- /.modal-dialog -->