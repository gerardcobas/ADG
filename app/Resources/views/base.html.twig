<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}{% endblock %} | {{ 'base.title' | trans }}</title>
          
        <link href="{{ asset('bundles/adg/css/font-awesome.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/adg/css/bootstrap.min.css') }}" rel="stylesheet">

	    
		<link href="{{ asset('bundles/adg/css/comu.css') }}" rel="stylesheet">

        {% block stylesheets %}{% endblock %}
		
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        
    </head>
    <body>
    
	     <div class="navbar navbar-inverse navbar-fixed-top " role="navigation">	
			    <div class="navbar-header">
				    <a class="navbar-brand" href="{{ path('index') }}"">{{ 'base.title' | trans }}</a>
					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
				      <span class="sr-only">Toggle navigation</span>
				      <span class="icon-bar"></span>
				      <span class="icon-bar"></span>
				      <span class="icon-bar"></span>
				    </button>
				</div>
				<div class="navbar-collapse collapse">
				      {{ knp_menu_render('PrincipalBundle:MenuBuilder:mainMenu', {'currentClass': 'active', 'template': 'PrincipalBundle:Menu:knp_menu.html.twig'}) }}
					  {{ knp_menu_render('PrincipalBundle:MenuBuilder:userMenu', {'currentClass': 'active', 'template': 'PrincipalBundle:Menu:knp_menu.html.twig'}) }}
				</div>
		 </div>
		{% block subnavbar %}{% endblock %}
		  <div class="content">
		    <div class="wrapper">
		      <div class="container proper-content">
		        {% block body %}{% endblock %}
		        <div class="space10"></div>
		      </div><!-- /.proper-content -->
		      <div class="push"></div>
		    </div><!-- /.wrapper -->
 			{% include '::footer.html.twig' %}
		  </div>

		
	    <script src="{{ asset('bundles/adg/js/jquery.js') }}" type="text/javascript"></script>
	    <script src="{{ asset('bundles/adg/js/bootstrap.min.js') }}" type="text/javascript"></script>	         
        {% block javascripts %}{% endblock %}


    </body>
</html>
